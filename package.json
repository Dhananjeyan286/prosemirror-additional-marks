{
  "name": "prosemirror-additional-marks",
  "version": "1.0.0",
  "description": "A prosemirror marks package to add additional marks into the editor",
  "main": "./dist/index.es.js",
  "type": "module",
  "exports": {
    "import": "./dist/index.es.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "demo": "npm run build --prefix demo",
    "dev": "rollup -c ./merge.config.js --dev",
    "build": "rollup -c ./merge.config.js",
    "check": "npm run dev && npm run demo"
  },
  "keywords": [
    "prosemirror",
